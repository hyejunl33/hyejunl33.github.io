# Gemfile

source 'https://rubygems.org'

gem 'jekyll', '~> 4.3.2' # Jekyll 최신 안정 버전 사용

group :jekyll_plugins do
  gem 'jekyll-feed'
  gem 'jekyll-sitemap'
  gem 'jekyll-redirect-from'
  gem 'jemoji'
  gem 'jekyll-seo-tag'
  # 필요에 따라 다른 플러그인을 추가할 수 있습니다.
end

# Jekyll 3.x 버전과의 호환성을 위한 webrick 추가
gem 'webrick', '~> 1.8'